  <% include headerAll %>
  <div class="wrap">
      <h1>New Brick Story Feed</h1>
      <form class="media_form" role='form' action="/updatestory" method="post">
        <fieldset class="form-group">
          <label for="story_title">Story Title</label>
          <input type="text" class="form-control" id="story_title" name="title" placeholder="Input story title">
 <!--          <small class="text-muted">We'll never share your email with anyone else.</small> -->
        </fieldset>
        <fieldset class="form-group">
            <label for="story_author">Author</label>
            <input type="text" class="form-control" id="story_author" placeholder="Author or source" name="author">
            <small class="text-muted">Please provide author name or cited source.</small>
        </fieldset>
         <fieldset class="form-group">
            <label for="story_imgsrc">Image Src</label>
            <input type="text" class="form-control" id="story_imgsrc" placeholder="image src" name="image_src">
            <small class="text-muted">Please provide image source hyperlink.</small>
        </fieldset>
        <fieldset class='form-group'>
           <div class='input-group date' >
                <label for="story_feeddate">Feed Date</label>
                <input type='text' class="form-control" id='datetimepicker1' name="feedDate">
                <span class="input-group-addon">
                        <span class="glyphicon glyphicon-calendar"></span>
                    </span>
            </div>
        </fieldset>

